#include <stdlib.h>
#include <stdio.h>
#include <string.h>
#include <time.h>
#include <math.h>
int main (int argc, char ** argv) {
	
	int n=1000,maxiter=1000;
	int j,k,t;
	double tol = 1.0e-3;
	double T[n+1][n+1];
	double tmp1[n],tmp2[n];
	char str[];


	int i;
    for(i=0; i < n+2; i++) {
        T[0][i] = 1.0;
        T[n+1][i] = 1.0;
        T[i][n+1] = 2.0;
    }
    T[0][n+1] = 2.0;

	return(0);
}
